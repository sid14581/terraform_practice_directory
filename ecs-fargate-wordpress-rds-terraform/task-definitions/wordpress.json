[
    {
        "name": "wordpress",
        "image": "wordpress:latest",
        "cpu": 0,
        "essential": true,
        "portMappings": [
            {
                "name": "wordpress-80-tcp",
                "hostPort": 80,
                "containerPort": 80,
                "protocol": "tcp",
                "appProtocol": "http"
            }
        ],
        "links": [],
        "command": [
        ],
        "entryPoint": [
        ],
        "environment": [
            {
                "name": "WORDPRESS_DB_HOST",
                "value": "${db_host}"
            },
            {
                "name": "WORDPRESS_DB_USER",
                "value": "${db_user}"
            },
            {
                "name": "WORDPRESS_DB_PASSWORD",
                "value": "${db_password}"
            },
            {
                "name": "WORDPRESS_DB_NAME",
                "value": "${db_name}"
            }
        ],
        "mountPoints": null,
        "volumesFrom": null,
        "extraHosts": null,
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-create-group": "true",
                "awslogs-group": "/ecs/wp-ecs-tf",
                "awslogs-region": "${region}",
                "awslogs-stream-prefix": "ecs"
            },
            "secretOptions": []
        },
        "ulimits": null,
        "dockerLabels": null,
        "systemControls": null
    }
]